{
  "name": "acromedia/drupalorange_profile",
  "type": "drupal-profile",
  "description": "Acro Media's Orange installation profile",
  "homepage": "https://github.com/AcroMedia/drupalorange_profile",
  "license": "GPL-2.0+",
  "require": {
    "drupal/admin_toolbar": "^1.15.0",
    "drupal/contact_storage": "^1.0.0-beta4",
    "drupal/ctools": "^3.0.0-alpha26",
    "drupal/google_analytics": "^2.0.0",
    "drupal/honeypot": "^1.23.0",
    "drupal/imce": "^1.2.0",
    "drupal/metatag": "dev-1.x",
    "drupal/pathauto": "^1.0.0-alpha3",
    "drupal/simple_sitemap": "^2.5.0",
    "drupal/taxonomy_menu": "dev-3.x",
    "drupal/token": "^1.0.0-alpha2",
    "drupal/userprotect": "^1.0.0-rc1",
    "acromedia/orange_starter": "dev-8.x-1.x",
    "acromedia/orange_framework": "dev-8.x-1.x"
  },
  "extra": {
    "installer-paths": {
      "web/profiles/drupalorange_profile/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/profiles/drupalorange_profile/modules/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/drupalorange_profile/themes/{$name}": [
        "type:drupal-theme"
      ]
    }
  }
}
